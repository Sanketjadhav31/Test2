pipeline {
    agent any
     tools{
        maven 'maven'
    }

    stages {
        stage('Git Checkout') {
            steps {
                git url: 'https://github.com/Sanketjadhav31/Devops.git', branch: 'master'
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy'
            }
        }
    }
}
